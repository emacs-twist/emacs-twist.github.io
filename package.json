{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format:prettier": "prettier --plugin-search-dir=. --write .",
    "lint:prettier": "prettier --plugin-search-dir=. --check .",
    "lint:eslint": "eslint --ext .ts,.astro src",
    "prepare": "husky install"
  },
  "dependencies": {
    "astro": "^2.3.0"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --plugin-search-dir=. --write",
      "eslint"
    ],
    "*.astro": [
      "prettier --plugin-search-dir=. --write",
      "eslint"
    ],
    "*.js?(on)": "prettier --plugin-search-dir=. --write"
  },
  "devDependencies": {
    "@astrojs/language-server": "^0.29.6",
    "@astrojs/ts-plugin": "^0.4.4",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-plugin-astro": "^0.26.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prettier-plugin-astro": "^0.8.0"
  }
}
